<!--pages/login/login.wxml-->
<view class="main">
  <view class="top-view">
  
  </view>
  
  <view class="midddle-view">
    <view class="logo-view">
      <text class="app-title-mid">{{title_name}}</text>
      <image class="title-logo" src="../../images/logo.png"></image>
    </view>
    <text class="app-des">{{title_desc}}</text>
    <image class="login-bg" mode="scaleToFill" src="../../images/bg.png"></image>
    <button class="login-btn" type="primary" bindtap="onLoginBtnTapped">{{btn_name}}</button>
  </view>
</view>

